    "undici": "^7.4.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^24.1.0",
    "eslint": "^9.20.0",
    "prettier": "^3.5.1",
    "tsx": "^4.20.5",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.24.0"
  }
}
